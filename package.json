{
  "name": "gym-website",
  "version": "0.1.0",
  "private": true,
"scripts": {
  "start": "cross-env NODE_ENV=production PORT=8080 node server.js", 
  "dev": "cross-env NODE_ENV=development PORT=3001 node server.js", 
  "cloudflare": "cloudflared tunnel --url http://localhost:8080",
  "wait-and-cloudflare": "wait-on http://localhost:8080 && npm run cloudflare",
  "start-prod": "concurrently \"npm run start\" \"npm run wait-and-cloudflare\"",
  "build": "next build"
},
